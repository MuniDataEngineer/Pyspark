{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Demoadfpipe"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"LS_ADLS_Raw_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_Raw'"
		},
		"LS_SQL_Inventory_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQL_Inventory'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqldb-retail-source.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "RetailDB"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Arun"
		},
		"LS_ADLS_Raw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://demostorge03.dfs.core.windows.net/"
		},
		"LS_SQL_Inventory_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqldb-retail-source.database.windows.net"
		},
		"LS_SQL_Inventory_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "onprem"
		},
		"LS_SQL_Inventory_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Arun"
		},
		"Rest_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dummyjson.com/products"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Demoadfpipe_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"LS_ADLS_Raw_accountKey": {
						"value": "[parameters('LS_ADLS_Raw_accountKey')]"
					},
					"LS_SQL_Inventory_password": {
						"value": "[parameters('LS_SQL_Inventory_password')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"LS_ADLS_Raw_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_Raw_properties_typeProperties_url')]"
					},
					"LS_SQL_Inventory_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_Inventory_properties_typeProperties_server')]"
					},
					"LS_SQL_Inventory_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_Inventory_properties_typeProperties_database')]"
					},
					"LS_SQL_Inventory_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_Inventory_properties_typeProperties_userName')]"
					},
					"Rest_api_properties_typeProperties_url": {
						"value": "[parameters('Rest_api_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}